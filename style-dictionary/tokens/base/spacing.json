    "sm": { "value": "calc(0.75 * var(--space-unit))" },
    "md": { "value": "calc(1 * var(--space-unit))" },
    "lg": { "value": "calc(1.5 * var(--space-unit))" },
    "xl": { "value": "calc(2 * var(--space-unit))" },
    "xxl": { "value": "calc(4 * var(--space-unit))" },
